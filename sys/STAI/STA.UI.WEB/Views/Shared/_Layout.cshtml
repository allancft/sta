<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    @{ ViewBag.Title = "STAI - Sistema de transmissão de arquivos Itambé"; }
    <title>@ViewBag.Title</title>
    @Html.Partial("_CSS") 
    <script src="../../Content/scripts/jquery.js" type="text/javascript"></script>
    <script src="../../Content/scripts/jquery-migrate-1.2.1.js" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <div class="panel panel-default cabecalho">
            <div class="cabecalho-logo">
                <img src="../../Content/images/logo-itambe.png" alt="Logo Itambé" class="img-responsive" />
            </div>
            <div class="cabecalho-titulo">
                @ViewBag.Title
            </div>
        </div>
        @RenderBody()
    </div>
    @Html.Partial("_JS")
    
    <div class="fixed-footer-mensagem">
        @Html.Partial("~/Views/Shared/_Mensagens.cshtml")
    </div>
    @*@Html.Partial("_ModalSair")*@
    @Html.Partial("_Loading")
</body>
</html>
